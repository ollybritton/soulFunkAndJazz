<?xml version='1.0' encoding='utf-8'?>
<widget id="net.soulfunkandjazz.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Soul Funk and Jazz Radio Player</name>
    <description>
        Plays the soul funk and jazz radio station.
    </description>
    <author email="admin@soulfunkandjazz.net" href="http://soulfunkandjazz.net">
        Olly Britton
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-mixSdkVersion" value="14" />
    <preference name="permissions" value="none" />
</widget>
